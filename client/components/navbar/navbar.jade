    ul#authenticatedNavigation.navCollection      
      li.navItem(ng-class="{active: isActive('/frames')}")
        a(ui-sref="frames") Dashboard
  
  .right
    ul#primaryNavigation.navCollection
      li.navItem(ng-repeat="item in menu", ng-class="{active: isActive(item.link)}")
        a(ng-href="{{item.link}}") {{item.title}}
  
      li.navItem(ng-show="isAdmin()", ng-class="{active: isActive('/admin')}")
        a(href="/admin") Admin
        
    ul#anonymous.navCollection(ng-show="!isLoggedIn()")
      li.navItem
        a(href="/login") Login
        
      li.navItem
        a(href="/signup") Sign up
  
    div#identity.navItem.hasImage(ng-show="isLoggedIn()")
      a
        img.image(ng-if="getCurrentUser().provider == 'facebook'", ng-src="http://graph.facebook.com/{{ getCurrentUser().facebook.id }}/picture?type=large")
        span.text {{ getCurrentUser().name }}
