div#dashboard-frames.layout-partial
	div.group
		div.organisation.row
			div.name.b3.uppercase.airy My wireframes
			
		div.frames.cards.row
			a.card(ng-repeat="frame in ProjectCtrl.project.frames | orderBy:'-created_at'", ui-sref="frame({ id: frame._id })")
				div.content
					div.left
						span.name.h4.block {{ frame.name }}
								
						ul.users
							li.user(ng-repeat="user in frame.users", uib-tooltip-html="'Shared with ' + user.name", tooltip-placement="bottom", tooltip-append-to-body="true", tooltip-popup-delay="800")
								ui-placeholder.avatar(type="user", src="user.image")
						
					div.right
						span.lastModified {{ (frame.created_at | moment).fromNow() }}
						
			a.card.new(ng-click="ProjectFramesCtrl.onNewFrameClick()")
				div.content
					span + add new
